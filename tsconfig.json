{
  "extends": "expo/tsconfig.base",  
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    ".expo/types/**/*.ts",
    ".expo/types/**/*.js",
    "expo-env.d.ts"
  ]
}
