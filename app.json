{
  "expo": {
    "name": "QuantumApp",
    "slug": "QuantumApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/quatum_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-quatum-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-quatum512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jonny200220.QuantumApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e362582d-70f5-407d-9df2-6af34dd3384b"
      }
    },
    "owner": "jonny200220"
  }
}
